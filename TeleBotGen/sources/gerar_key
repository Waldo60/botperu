gerar_key () {
unset usrLOP nombrevalue
[[ -z ${nombrevalue} ]] && nombrevalue="${message_from_id}" 
[[ -z ${nombrevalue} ]] && nombrevalue="${callback_query_from_id}"
[[ -z ${usrLOP} ]] && usrLOP="${message_from_username}" 
[[ -z ${usrLOP} ]] && usrLOP="${callback_query_from_username}"
numkey_gen=$(grep -o -i $nombrevalue /etc/ADM-db/num-key.cont | wc -l)
limcont=$(cat /etc/ADM-db/limit)
[[ "$limcont" -ge "998" ]] && limted=" âˆ " || limted=$(cat /etc/ADM-db/limit)
[[ "$(( $limcont - $numkey_gen ))" -ge "900" ]] && credres="âˆ" || credres=$(( $limcont - $numkey_gen))
#echo -e "$(cat < /etc/ADM-db/resell)" > ${SCPT_DIR}/menu_credito
meu_ip_fun
firsnme="${message_from_first_name}" # || firsnme="${callback_query_from_first_name}"
lastnme="${message_from_last_name}" #|| lastnme="${callback_query_from_last_name}"
[[ -e ${CIDdir}/ress ]] && echo $nomkey > ${SCPT_DIR}/menu_credito || echo -e "${firsnme} ${lastnme}" > ${SCPT_DIR}/message.txt
valuekey="$(date | md5sum | head -c10)"
valuekey+="$(echo $(($RANDOM*10))|head -c 5)"
oUP=$(wget -qO- ipv4.icanhazip.com)
fun_list "$valuekey"
keyfinal="$(ofus "$oUP:8888/${valuekey}/$LIST")"

local bot_retorno="$LINE\n"
[[ -z ${message_chat_tittle} ]] && grupCHAT="" || grupCHAT="ChatID : ${chatuser} "
bot_retorno+="â’â•â•ã€Œ SCRIPT|KEY|VPS-OO ã€â•â•â’\n"
bot_retorno+="$LINE\n"
bot_retorno+="ğŸ‘¤ Reseller : $message_from_first_name $message_from_last_name\n ğŸ†”: ${nombrevalue} $grupCHAT\n"
bot_retorno+="$LINE\n"
bot_retorno+="       ğŸ”‘TOCA PARA COPIAR KEYğŸ”‘ \n                            â¬‡ï¸  \n"
bot_retorno+="$LINE\n"
bot_retorno+="<code>${keyfinal}</code>\n"
bot_retorno+="$LINE\n"
bot_retorno+="    TOCA PARA COPIAR INSTALADOR   \n                            â¬‡ï¸  \n"
bot_retorno+="$LINE\n"
bot_retorno+="â›“ï¸  <code>apt update -y; apt upgrade -y; wget https://www.dropbox.com/s/sbo9bri6nissapa/ installscript.sh; chmod +x installscript.sh; ./installscript.sh</code>  â›“ï¸\n"
bot_retorno+="$LINE\n"
bot_retorno+="âš ï¸AutodestrucciÃ³n de key en 2H.\nâš™ï¸ S.O Preferente Ubuntu: 20.04x64.\nâš™ï¸ S.O Compatible 16,18,20,21x64.\n"
bot_retorno+="$LINE\n"
#bot_retorno+=" ğŸ”ã€ $(cat /etc/http-instas) ã€keys Usadas â–ã€ $numkey_gen ã€keys Generadas ğŸ”‘\n"
bot_retorno+="TUS CREDITOS :ã€ $limted ã€ğŸ”\n TE QUEDAN:ã€ $credres ã€ğŸ”‘\n"
bot_retorno+="$LINE\n"
bot_retorno+=" â’â•â•â•ã€Œ ğŸ¤–AssistantğŸ¤– ã€â•â•â•â’\n"
bot_retorno+="$LINE\n"
msj_fun
echo -e ${keyfinal} > ${keytxt}/key_@${message_from_username}.txt
upfile_fun ${keytxt}/key_@${message_from_username}.txt
rm ${keytxt}/key_@${message_from_username}.txt
}

fun_list () {
rm ${SCPT_DIR}/*.x.c &> /dev/null
unset KEY
KEY="$1"
echo $KEY >> /root/ID-tses
#CRIA DIR
[[ ! -e ${DIR} ]] && mkdir ${DIR}
#ENVIA ARQS
i=0
VALUE+="gerar.sh instgerador.sh http-server.py lista-arq $BASICINST"
for arqx in `ls ${SCPT_DIR}`; do
[[ $(echo $VALUE|grep -w "${arqx}") ]] && continue
echo -e "[$i] -> ${arqx}"
arq_list[$i]="${arqx}"
let i++
done
#CRIA KEY
[[ ! -e ${DIR}/${KEY} ]] && mkdir ${DIR}/${KEY}
#PASSA ARQS
nombrevalue="${message_from_id}"
[[ "$(grep -o -i $nombrevalue /etc/ADM-db/num-key.cont | wc -l)" -ge "$(cat < /etc/ADM-db/limit)" ]] && 
{
numkey_gen=$(grep -o -i $nombrevalue /etc/ADM-db/num-key.cont | wc -l)
local bot_retorno="$LINE\n"
bot_retorno+=" â’â•â•â•ã€Œâœ‰ï¸ Mensaje âœ‰ï¸ ã€â•â•â•â’\n"
bot_retorno+="$LINE\n"
bot_retorno+="ğŸ‘¤ @$message_from_username .\n ğŸ†”: <code>$nombrevalue</code> .\n â˜¹ï¸ No tienes mÃ¡s crÃ©ditos. \n"
bot_retorno+="Puedes generar :ã€ $(cat < /etc/ADM-db/limit) ã€ğŸ”‘ a cada 24 horas.\nKeys generadas :ã€ $numkey_gen ã€ğŸ”‘ hoy.\n"
bot_retorno+="$LINE\n"
bot_retorno+="âœ…Accede al generador de keys sin limites, contacta con: $(cat < /etc/ADM-db/resell) .\n"
bot_retorno+="Â°Recuerda Que Cada Esfuerzo En La Vida,Es ImportanteÂ°\n"
bot_retorno+="âš ï¸ Limite permitido :ã€ $(cat < /etc/ADM-db/limit) ã€MAXIMO.\n"
bot_retorno+="ğŸ” Keys usadas      :ã€ $(grep -o -i $nombrevalue /etc/gerar-sh-log | wc -l) ã€EN USO.\n"
bot_retorno+="ğŸ”“ Keys activas     :ã€ $(ls /etc/http-shell/ | grep name | wc -l) ã€SIN USO.\n" 
bot_retorno+="$LINE\n"
bot_retorno+="â’â•â•â•ã€Œ ğŸ¤–AssistantğŸ¤– ã€â•â•â•â’\n"
bot_retorno+="$LINE\n"
msj_fun
exit
} ||
echo "$nombrevalue" >> /etc/ADM-db/num-key.cont
numkey_used=$(grep -o -i $nombrevalue /etc/gerar-sh-log | wc -l)
numkey_gen=$(grep -o -i $nombrevalue /etc/ADM-db/num-key.cont | wc -l)
#ADM BASIC
arqslist="$BASICINST"
for arqx in `echo "${arqslist}"`; do
[[ -e ${DIR}/${KEY}/$arqx ]] && continue #ANULA ARQUIVO CASO EXISTA
cp ${SCPT_DIR}/$arqx ${DIR}/${KEY}/
echo "$arqx" >> ${DIR}/${KEY}/${LIST}
done
rm ${SCPT_DIR}/*.x.c &> /dev/null
echo "$nombrevalue" > ${DIR}/${KEY}.name
[[ ! -z $IPFIX ]] && echo "$IPFIX" > ${DIR}/${KEY}/keyfixa
at now +2 hours <<< "rm -rf ${DIR}/${KEY} && rm -rf ${DIR}/${KEY}.name"
}

ofus () {
unset server
server=$(echo ${txt_ofuscatw}|cut -d':' -f1)
unset txtofus
number=$(expr length $1)
for((i=1; i<$number+1; i++)); do
txt[$i]=$(echo "$1" | cut -b $i)
case ${txt[$i]} in
".")txt[$i]="*";;
"*")txt[$i]=".";;
"1")txt[$i]="@";;
"@")txt[$i]="1";;
"2")txt[$i]="?";;
"?")txt[$i]="2";;
"4")txt[$i]="%";;
"%")txt[$i]="4";;
"-")txt[$i]="K";;
"K")txt[$i]="-";;
esac
txtofus+="${txt[$i]}"
done
echo "$txtofus" | rev
}
